{
  "name": "snowpack-template-react-typescript-twinmacro-overmind",
  "description": "A preconfigured template for Snowpack with React, Typescript, Twin.Macro and Overmind",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/breadbeard/snowpack-template-react-typescript-twinmacro-overmind#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/breadbeard/snowpack-template-react-typescript-twinmacro-overmind.git"
  },
  "keywords": [
    "csa-template"
  ],
  "publishConfig": {
    "access": "public"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.2",
    "@emotion/styled": "^11.0.0",
    "overmind": "^26.0.0",
    "overmind-devtools": "^27.0.0",
    "overmind-react": "^27.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "tailwindcss": "^2.0.2",
    "twin.macro": "^2.0.7"
  },
  "devDependencies": {
    "@snowpack/app-scripts-react": "^1.12.6",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-react-refresh": "^2.3.9",
    "@snowpack/plugin-typescript": "^1.1.1",
    "@snowpack/web-test-runner-plugin": "^0.1.5",
    "@testing-library/react": "^11.0.0",
    "@types/chai": "^4.2.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/snowpack-env": "^2.3.2",
    "@web/test-runner": "^0.10.0",
    "chai": "^4.2.0",
    "prettier": "^2.0.5",
    "rollup": "^2.35.1",
    "snowpack": "^2.18.4",
    "typescript": "^4.0.0"
  }
}
